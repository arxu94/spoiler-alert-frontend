/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 


.home-button {
  margin: auto 0;
  border: solid 4rpx #f8983e;
  box-shadow: 0 0 8rpx rgba(0,0,0,0.3);
  border-radius: 10rpx;
  color: rgb(241, 241, 241);
  background-color: #F88B26;
  text-align: center;
  display: block;
  margin: 30rpx auto;
  width: 50px;
  height: 50px;
  border-radius: 100%;
  position: sticky;
  flex-wrap: wrap;
  z-index: 1;
  bottom: 10px;
}

.home-button-icon {
  height: 30px;
  width: 30px;
  padding-top: 17rpx;
  text-align: center;
  align-items: center;
}
